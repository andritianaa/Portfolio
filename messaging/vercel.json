{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/messages",
            "dest": "/messages"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}